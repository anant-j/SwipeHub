{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style":"off",
        "no-unused-vars": "off",
        "no-undef": "off",
        "max-len": ["error", {"code": 400}],
        "require-jsdoc": "off"
    }
}
